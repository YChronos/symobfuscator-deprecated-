  %cmp = icmp slt i32 %4, 10
  br i1 %cmp, label %for.body, label %for.end

for.body:                                         ; preds = %for.cond
  %5 = load i8, i8* %plaintext, align 1
  %conv4 = sext i8 %5 to i32
  %add5 = add nsw i32 %conv4, 2
