  %3 = load i8, i8* %ciphertext, align 1
  %conv = sext i8 %3 to i32
  %add = add nsw i32 %conv, 7
  %conv2 = trunc i32 %add to i8
  store i8 %conv2, i8* %plaintext, align 1
  store i32 0, i32* %i, align 4
  br label %for.cond

for.cond:                                         ; preds = %for.inc, %entry
