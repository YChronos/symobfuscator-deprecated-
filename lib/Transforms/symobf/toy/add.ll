source_filename = "add.c"
target datalayout = "e-m:e-i64:64-f80:128-n8:16:32:64-S128"
target triple = "x86_64-unknown-linux-gnu"

@.str = private unnamed_addr constant [4 x i8] c"%d\0A\00", align 1
@.str.1 = private unnamed_addr constant [4 x i8] c"%c\0A\00", align 1

; Function Attrs: nounwind uwtable
define i32 @main(i32 %argc, i8** %argv) #0 {
entry:
  %argc.addr = alloca i32, align 4
  %argv.addr = alloca i8**, align 8
  %ciphertext = alloca i8, align 1
  %plaintext = alloca i8, align 1
  %a = alloca i32, align 4
  store i32 %argc, i32* %argc.addr, align 4
  store i8** %argv, i8*** %argv.addr, align 8
  %0 = load i8**, i8*** %argv.addr, align 8
  %arrayidx = getelementptr inbounds i8*, i8** %0, i64 1
  %1 = load i8*, i8** %arrayidx, align 8
  %arrayidx1 = getelementptr inbounds i8, i8* %1, i64 0
  %2 = load i8, i8* %arrayidx1, align 1
  store i8 %2, i8* %ciphertext, align 1
  %3 = load i8, i8* %ciphertext, align 1
  %conv = sext i8 %3 to i32
  %add = add nsw i32 %conv, 7
  %conv2 = trunc i32 %add to i8
  store i8 %conv2, i8* %plaintext, align 1
  store i32 2, i32* %a, align 4
  %4 = load i32, i32* %a, align 4
  %call = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @.str, i32 0, i32 0), i32 %4)
  %5 = load i8, i8* %plaintext, align 1
  %conv3 = sext i8 %5 to i32
  %call4 = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @.str.1, i32 0, i32 0), i32 %conv3)
  ret i32 0
}

declare i32 @printf(i8*, ...) #1

